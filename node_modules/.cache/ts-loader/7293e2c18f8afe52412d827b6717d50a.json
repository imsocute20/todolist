,GAAG,CAAC,IAAD,EAAO,IAAP,EAAa,IAAb,CAAjB;AACA,aAAO,QAAQ,CAAC,MAAD,CAAf;AACD,K,CAED;;;;WACA,cAAK,IAAL,EAAmB;AACjB;AACA,UAAI,QAAQ,GAAyB,KAAK,QAAL,CAAc,IAAd,CAAmB,UAAC,GAAD,EAAQ;AAC9D,eAAO,GAAG,CAAC,EAAJ,IAAU,IAAI,CAAC,EAAtB;AACD,OAFoC,CAArC,CAFiB,CAKjB;;AACA,UAAI,QAAJ,EAAc;AACZ,QAAA,QAAQ,CAAC,UAAT,GAAsB,IAAI,CAAC,UAA3B;AACA,QAAA,QAAQ,CAAC,KAAT,GAAiB,IAAI,CAAC,KAAtB;AACA,QAAA,QAAQ,CAAC,OAAT,GAAmB,IAAI,CAAC,OAAxB,CAHY,CAKZ;;AACA,aAAK,UAAL,CAAgB,QAAhB,CAAyB,KAAK,QAA9B;AACD;AACF,K,CAED;;;;WACA,gBAAO,EAAP,EAAiB;AACf;AACA,UAAI,KAAK,GAAW,KAAK,QAAL,CAAc,SAAd,CAAwB,UAAC,GAAD,EAAQ;AAClD,eAAO,GAAG,CAAC,EAAJ,KAAW,EAAlB;AACD,OAFmB,CAApB,CAFe,CAMf;;AACA,UAAI,KAAK,GAAG,CAAC,CAAb,EAAgB;AACd,aAAK,QAAL,CAAc,MAAd,CAAqB,KAArB,EAA4B,CAA5B,EADc,CAEd;;AACA,aAAK,UAAL,CAAgB,QAAhB,CAAyB,KAAK,QAA9B;AACD;AACF;;;;;;AAIH,eAAe,YAAf","sourcesContent":["import DataHelper from './DataHelper'\r\nimport ItemData from '../model/ItemData'\r\nimport Category from '@/model/CateEnum'\r\nclass ActionHelper {\r\n  //1.负责数据处理 -------------------------------------------------------\r\n  dataHelper: DataHelper = new DataHelper('memoData', 'id');\r\n  //1.1 笔记数组\r\n  memoList!: Array<ItemData>;\r\n\r\n  // 构造函数：读取本地数据，并设置给 成员变量 memoList\r\n  constructor() {\r\n    //读取本地数据，将 笔记数组 保存 到 this.memoList 变量中\r\n    this.memoList = this.readData();\r\n  }\r\n\r\n  // 读取本地数据，并返回 ItemData类型数组\r\n  readData(): Array<ItemData> {\r\n    //1.读取 本地 的 Object数组 - dataHelper\r\n    let arrObj = this.dataHelper.readData();\r\n\r\n    //2.将 Object数组 转成 ItemData数组\r\n    let arrItem = arrObj.map((ele: any) => {\r\n\r\n      let item: ItemData = new ItemData();\r\n\r\n      item.id = ele.id;\r\n      item.categoryId = ele.categoryId;\r\n      item.title = ele.title;\r\n      item.content = ele.content;\r\n      item.createTime = ele.createTime;\r\n\r\n      return item;\r\n    });\r\n\r\n    //3.返回itemData数组\r\n    return arrItem;\r\n  }\r\n\r\n  //2.负责业务处理 -------------------------------------------------------\r\n  //2.1 新增笔记\r\n  add(item: ItemData): number {\r\n    //a.保存到本地 dataHelper.addData ，会返回 生成的 id值\r\n    item.id = this.dataHelper.addData(item);\r\n    //b.将笔记添加到 笔记数组中\r\n    this.memoList.push(item);\r\n    //d.返回 新笔记的 id\r\n    return item.id;\r\n  }\r\n\r\n  getCategoryName(cateId: Category): string {\r\n    const arrNames = ['工作', '生活', '学习'];\r\n    return arrNames[cateId];\r\n  }\r\n\r\n  //2.2 修改笔记\r\n  edit(item: ItemData): void {\r\n    //a.找出 数组中 id 相同 的对象\r\n    let editItem: ItemData | undefined = this.memoList.find((ele) => {\r\n      return ele.id == item.id;\r\n    });\r\n    //b.修改 对象的值：将 传入对象item各个属性的值 覆盖设置 给 数组中找到对象的属性\r\n    if (editItem) {\r\n      editItem.categoryId = item.categoryId;\r\n      editItem.title = item.title;\r\n      editItem.content = item.content;\r\n\r\n      //c.将更新后的 数组 重新保存到 本地localstorage\r\n      this.dataHelper.saveData(this.memoList);\r\n    }\r\n  }\r\n\r\n  //2.3 删除笔记\r\n  remove(id: number): void {\r\n    //a.根据id 找出 要删除的 对象 在数组中的 下标\r\n    let index: number = this.memoList.findIndex((ele) => {\r\n      return ele.id === id;\r\n    })\r\n\r\n    //b.根据下标 调用 数组.splice方法来删除对象\r\n    if (index > -1) {\r\n      this.memoList.splice(index, 1);\r\n      //c.将删除对象后的 数组重新保存回 localstorage\r\n      this.dataHelper.saveData(this.memoList);\r\n    }\r\n  }\r\n\r\n}\r\n\r\nexport default ActionHelper;"],"sourceRoot":""}]}